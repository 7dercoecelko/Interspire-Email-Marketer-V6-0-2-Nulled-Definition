<div class="Container Container--full">
	<div class="Grid">

		<article class="Grid-cell Grid-cell--triple">
			{{channel-header channel=model}}
		</article>

		{{#if model.canEdit}}
			<div class="Grid-cell Grid-cell--triple">
				<form class="Form">
					{{track-add-inline addTrackUrl=addTrackUrl
						onSubmit=(action "addTrack")}}
				</form>
			</div>

			{{#unless model.isExperienced}}
				<div class="Grid-cell Grid-cell--triple">
					{{page-welcome channel=model}}
				</div>
			{{/unless}}
		{{/if}}

	</div>
</div>

{{outlet}}
