<div class="Channel-outlet">
	{{#if model.channelPublic.followers}}

		<div class="Container Section">
			<p>{{model.channelPublic.followers.length}} followers.</p>
		</div>
		{{channels-list channels=model.channelPublic.followers}}

	{{else}}

		<div class="Container Section">
			<p>This channel doesn't have any followers, yet.</p>
		</div>

	{{/if}}
</div>
